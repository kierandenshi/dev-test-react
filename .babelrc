{
  "presets": [
    ["@babel/preset-env"],
    ["@babel/preset-react"]
  ],
  "plugins": [
    ["babel-plugin-styled-components", {
      "ssr": true
    }],
    ["@babel/plugin-proposal-class-properties"],
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "@api": "./src/api",
        "@components": "./src/components",
        "@config": "./src/config",
        "@redux": "./src/redux"
      }
    }]
  ]
}
